{{'''SELECT distinct ?class ?property ?propertyType ?inverse (count(distinct ?s) as ?count) (sample(?facetId) as ?facetId) WHERE {
  ?subject prov:specializationOf? ?s.
  ?s rdf:type/rdfs:subClassOf* ?class.
  {
    ?s ?property ?o.
    filter (isLiteral(?o))
    bind(owl:DatatypeProperty as ?propertyType)
    bind(datatype(?o) as ?range)
  } union {
    ?s ?property ?o.
    filter (!isLiteral(?o))
    optional {
      ?property owl:inverseOf|^owl:inverseOf ?inverse.
    }
    minus {?s rdf:type ?o}
    bind(owl:ObjectProperty as ?propertyType)
  }
  bind(?property as ?facetId)
} group by ?class ?property ?label ?propertyType ?inverse order by desc(?count)
LIMIT 100'''|query(values={"class":this.identifier}) | iter_labelize('property','label') | map_list('property','facetId',g.slugify) | tojson }}
