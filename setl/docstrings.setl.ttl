:docstrings_file a setl:JSON;
  api_vocab:selector "item";
  prov:wasGeneratedBy [
    a setl:Extract;
    prov:used  [ a graph4code:DocStringsFile];
  ].

:docstrings_import rdfs:subClassOf setl:SemanticETLScript;
  rdfs:label "Graph4code Import SETL Script";
  setl:hasTemplatePrefix "http://purl.org/twc/graph4code/setl/";
  rdfs:subClassOf [ a owl:Restriction;
    owl:onProperty prov:used;
    owl:someValuesFrom graph4code:DocStringsFile
  ].


